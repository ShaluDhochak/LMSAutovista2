<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/dseWiseList_RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp">

        <TextView
            android:id="@+id/dseNameListReport_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:text="name"
            android:paddingTop="5dp"
            android:textSize="14sp"
            android:textColor="@color/dark_grey"
            />

        <TextView
            android:id="@+id/dseLnameListReport_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/contact_no"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:textSize="14sp"
            android:textColor="@color/dark_grey"
            android:layout_below="@id/dseNameListReport_TextView"
            />

        <TextView
            android:id="@+id/viewDetailsDseList_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/outline_blue_curved_rectangle"
            android:layout_alignParentRight="true"
            android:text="View Details"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:textColor="@color/darkBlueWebsite"
            android:textSize="14sp"
            />

    </RelativeLayout>
</RelativeLayout>