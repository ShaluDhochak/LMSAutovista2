            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            android:layout_below="@+id/exchangeMakeModelSearch_linearlayout"
            >
            <TextView
                android:id="@+id/manufacturingYearSearchHeading_textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/manufacturingYear"
                android:textSize="12sp"
                android:textColor="@color/edittextcolor"
                />
            <TextView
                android:id="@+id/manufacturingYearSearchString_textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:hint="@string/NotMentioned"
                android:textSize="12sp"
                android:textColor="@color/edittextcolor"
                />
        </LinearLayout>

        <View
            android:id="@+id/separatorContactNoSearch_View"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/lightGrey1"
            android:layout_below="@+id/manufacturingYearSearch_linearlayout"
            />
    </RelativeLayout>

</LinearLayout>